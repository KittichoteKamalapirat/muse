    "@typescript-eslint/parser": "^5.17.0",
    "cypress": "^9.5.4",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "expo-yarn-workspaces": "^1.7.0",
    "gen-env-types": "^1.3.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.12",

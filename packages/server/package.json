{
  "name": "server",
  "version": "1.0.24",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon dist/index.js",
    "dev:test": "NODE_ENV=test nodemon dist/index.js",
    "typeorm:gen": "ts-node ./../../node_modules/typeorm/cli.js migration:generate -n migrationName -p",
    "gen-env": "gen-env-types .env -o src/env.d.ts -e .",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint . --ext .ts,.tsx",
    "check-types": "tsc --noemit",
    "test-server": "NODE_ENV=test ts-node src/__test__/utils/setup.ts",
    "test": "NODE_ENV=test jest ",
    "start": "node dist/index.js",
    "start2": "ts-node src/index.ts",
    "postinstall": "expo-yarn-workspaces postinstall",
    "start:cixx": "(NODE_ENV=development PORT=4000 node dist/index.js &) >/dev/null & wait-on http://localhost:4000",
    "start:ci": "(NODE_ENV=development node --trace-warnings dist/index.js&) >/dev/null ",
    "ci:test": "jest NODE_ENV=test jest --ci --runInBand --detectOpenHandles --forceExit",
    "server-for-cypress": "NODE_ENV=test node dist/index.js",
    "cypress:open": "NODE_ENV=test yarn cypress open --project ./src",
    "cypress:run": "NODE_ENV=test yarn cypress run --project ./src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",

    "moment": "^2.29.1",
    "node-fetch": "^2.0.0",
    "nodemailer": "^6.6.3",
    "pg": "^8.7.1",
    "promptpay-qr": "^0.4.4",
    "qrcode": "^1.4.4",
    "reflect-metadata": "^0.1.13",
